{
  "name": "start-browserify-project",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "suxxus",
  "license": "ISC",
  "literalify": {},
  "description": "react project starter",
  "prj": {
    "title": "title",
    "dependencies": [],
    "styledependencies": [
      "main.css"
    ]
  },
  "scripts": {
    "lint": "eslint --color  src || true",
    "test": "NODE_PATH=src babel-node src/tests | tap-colorize",
    "clean": "rm -rf ./build/* && rm -rf .tmp/*  && rm -rf ./dist/*",
    "prebuild": "npm run clean -s",
    "build:js": "NODE_PATH=src browserify -d -t babelify -t literalify src/scripts/index.js > build/bundle.js",
    "build:markup": "jade src/index.jade --obj package.json --pretty --out build",
    "build:css": "cat src/styles/*.css > build/main.css",
    "build": "npm test -s & npm run lint -s & npm run build:js -s & npm run build:css -s & npm run build:markup -s",
    "browsersync": "browser-sync start --reload-delay 2000 --files './build/*.js' --server ./build",
    "storybook": "start-storybook -p 9001",
    "dev": "watch -p 'src/**/*' -c 'npm run test & npm run lint'",
    "start": "npm run build && npm run browsersync & watch -p 'src/**/*' -c 'npm run build'",
    "uglify": "uglifyjs -m -c build/*.js -o dist/bundle.js",
    "minify:html": "html-minifier --collapse-whitespace build/index.html -o  dist/index.html",
    "dist": "npm run lint & npm run test && npm run build && cat build/*.css | cleancss -o dist/main.css && npm run minify:html && npm run uglify",
    "check": "npm outdated --depth=0",
    "update": "updtr"
  },
  "devDependencies": {
    "@kadira/storybook": "1.12.0",
    "autoprefixer-core": "6.0.1",
    "babel-cli": "6.7.7",
    "babel-eslint": "6.0.2",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babelify": "7.3.0",
    "browser-sync": "2.12.3",
    "browserify": "13.0.0",
    "clean-css": "3.4.12",
    "css-loader": "0.23.1",
    "enzyme": "2.2.0",
    "eslint": "2.8.0",
    "eslint-cli": "1.1.0",
    "eslint-config-eslint": "3.0.0",
    "eslint-plugin-react": "5.0.0",
    "html-minifier": "1.5.0",
    "jade": "1.11.0",
    "literalify": "0.4.0",
    "mock": "0.1.1",
    "postcss-color-rebeccapurple": "2.0.0",
    "postcss-loader": "0.8.2",
    "react-addons-test-utils": "15.0.1",
    "sinon": "1.17.3",
    "style-loader": "0.13.1",
    "tap-colorize": "1.2.0",
    "tape": "4.5.1",
    "uglify": "0.1.5",
    "updtr": "0.1.10",
    "watch-cli": "0.2.1"
  },
  "keywords": [
    "node",
    "project",
    "browserify"
  ],
  "dependencies": {
    "react": "15.0.1",
    "react-dom": "15.0.1"
  }
}
